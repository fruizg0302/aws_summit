<p id="notice"><%= notice %></p>

<h1>Awards</h1>

<table>
  <thead>
    <tr>
      <th>Contact email</th>
      <th>Answer 1</th>
      <th>Answer 2</th>
      <th>Answer 3</th>
      <th>Answer 4</th>
      <th>Answer 5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awards.each do |award| %>
      <tr>
        <td><%= award.contact_email %></td>
        <td><%= award.answer_1 %></td>
        <td><%= award.answer_2 %></td>
        <td><%= award.answer_3 %></td>
        <td><%= award.answer_4 %></td>
        <td><%= award.answer_5 %></td>
        <td><%= link_to 'Show', award %></td>
        <td><%= link_to 'Edit', edit_award_path(award) %></td>
        <td><%= link_to 'Destroy', award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Award', new_award_path %>
